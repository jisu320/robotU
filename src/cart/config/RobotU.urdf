<?xml version="1.0" ?>
  <robot name="RobotU">
  <link name="base_link">
    <visual>
      <geometry>
          <box size="0.39 0.34 0.39"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  <inertial>
            <mass value="3" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
          <box size="0.39 0.34 0.39"/>
      </geometry>

    </collision>
  </link>



  <link name="wheel_right_link">
    <inertial>
            <mass value="2" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>

      <geometry>
          <cylinder radius="0.1125" length="0.05"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>

      <geometry>
          <cylinder radius="0.1125" length="0.05"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <origin xyz="0.00 0.195 -0.1125" rpy="1.57 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>

  <link name="wheel_left_link">
    <inertial>
            <mass value="2" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>

      <geometry>
          <cylinder radius="0.1125" length="0.05"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <cylinder radius="0.1125" length="0.05"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <origin xyz="0.00 -0.195 -0.1125" rpy="1.57 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="wheel_left_link"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>



  <link name="caster">
    <inertial>
            <mass value="0.5" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>

      <geometry>
        <sphere radius="0.03" />
      </geometry>
      <material name="white" />
    </visual>

    <collision>
      <origin/>
      <geometry>
        <sphere radius=".03" />
      </geometry>
    </collision>
  </link>

  <joint name="caster_joint" type="continuous">
    <origin xyz="-0.15 0.0 -0.195" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1" />
    <parent link="base_link"/>
    <child link="caster"/>
  </joint>
  <link name="caster2">
    <inertial>
            <mass value="0.5" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>

      <geometry>
        <sphere radius="0.03" />
      </geometry>
      <material name="white" />
    </visual>

    <collision>
      <origin/>
      <geometry>
        <sphere radius=".03" />
      </geometry>
    </collision>
  </link>

  <joint name="caster_joint2" type="continuous">
    <origin xyz="0.15 0.0 -0.195" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1" />
    <parent link="base_link"/>
    <child link="caster2"/>
  </joint>



  <link name="lidar">
    <inertial>
            <mass value="0.5" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>

      <geometry>
        <cylinder radius="0.03" length="0.02"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>

  <joint name="lidar_joint" type="fixed">
    <origin xyz="0.15 0 0.196" rpy="0 0.0 1.57"/>
    <parent link="base_link"/>
    <child link="lidar"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>


</robot>